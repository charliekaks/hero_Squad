
#if ($heros)
    #foreach ($hero in $heros)
    <div class="well">
      <p>$hero.getName()</p>
      <p>$hero.getAge()</p>
      <p>$hero.getSuperPower()</p>
      <p>$hero.getWeakness()</p>
      <li><a href="/heroes/$hero.getId()">$hero.getName()</a></li>
    </div>
